{
  "name": "cucumber-screenplay-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm install && react-scripts start",
    "run": "npm install && BROWSER=none react-scripts start",
    "build": "npm install && react-scripts build",
    "test": "npm install && react-scripts test",
    "eject": "npm install && react-scripts eject"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
